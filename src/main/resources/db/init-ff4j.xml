<?xml version="1.0" encoding="UTF-8" ?>
<ff4j xmlns="http://www.ff4j.org/schema/ff4j"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.ff4j.org/schema/ff4j http://ff4j.org/schema/ff4j-1.4.0.xsd">

    <features>
        <feature uid="new_cars_list" enable="false"  description="New cars list table" />

        <feature uid="new_cars_list_weight" enable="true" description="New cars list table">
            <flipstrategy class="org.ff4j.strategy.PonderationStrategy" >
                <param name="weight" value="0.6" />
            </flipstrategy>
        </feature>

        <feature uid="new_cars_new" enable="false"  description="New cars form" />

        <feature uid="new_cars_new_weight" enable="true" description="New cars form">
            <flipstrategy class="org.ff4j.strategy.PonderationStrategy" >
                <param name="weight" value="0.2" />
            </flipstrategy>
        </feature>

        <feature uid="new_cars_edit" enable="false"  description="New edit cars form" />

        <feature uid="new_cars_edit_weight" enable="true" description="New edit cars form">
            <flipstrategy class="org.ff4j.strategy.PonderationStrategy" >
                <param name="weight" value="0.2" />
            </flipstrategy>
        </feature>

    </features>
</ff4j>